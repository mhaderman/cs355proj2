<%- include("../includes/header.ejs", {title: "Add"}); %>

<p>Add new Game:</p>
<form action="/game/addGame" method="get">
    <p>game_id (+1 of last game_id)</p>
    <input id="game_id" type="text" name="game_id">
    <p>date (00-00-0000)</p>
    <input id="date" type="text" name="date">
    <p>opponent</p>
    <input id="opponent" type="text" name="opponent">
    <p>win_lose (1 or 0)</p>
    <input id="win_lose" type="text" name="win_lose">
    <p>score (#-#)</p>
    <input id="score" type="text" name="score">

    <button type="'submit">Submit Game</button>
</form>